{
  "name": "bear-element",
  "version": "1.0.2",
  "description": "Bearmute Element",
  "type": "module",
  "main": "./dist/umd/v-element.umd.cjs",
  "author": "mrrdxx",
  "license": "MIT",
  "keywords": [
    "Vue3",
    "Component",
    "UI"
  ],
  "module": "./dist/es/v-element.js",
  "files": [
    "dist"
  ],
  "homepage": "http://element.vikingship.xyz/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vikingmute/y-element/"
  },
  "sideEffects": [
    "dist/v-element.css"
  ],
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/v-element.js",
      "require": "./dist/umd/v-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/v-element.css dist/v-element.css",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.9.0",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.25.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^7.0.2",
    "postcss-each": "^1.1.0",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.5.3",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.31.0",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.2",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  }
}
